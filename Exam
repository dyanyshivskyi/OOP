class MyClass:
    

    attribute1 : str
    attribute2 : int

    __init__(self, attribute1, attribute2):
    get_info(self):

    def __init__(self, attribute1, attribute2):

        attribute1 : str
        attribute2 : int
        self.attribute1 = attribute1
